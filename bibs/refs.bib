journal = {Transportation Research Part C: Emerging Technologies},
keywords = {Connected vehicle,GPS trajectory,Mobile data,Safety Pilot Model Deployment (SPMD) project,Traffic signal,Traffic volume estimation,Vehicle-to-infrastructure communication},
mendeley-groups = {Trafic flow: RL},
pages = {347--362},
publisher = {Elsevier Ltd},
title = {{Estimating traffic volumes for signalized intersections using connected vehicle data}},
url = {http://dx.doi.org/10.1016/j.trc.2017.03.007},
volume = {79},
year = {2017}
}
